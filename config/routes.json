{
  "GuriZone/": {
    "controller": "Producto",
    "method": "index"
  },
  "GuriZone/catalogo": {
    "controller": "Producto",
    "method": "catalogo"
  },
  "GuriZone/producto/:id": {
    "controller": "Producto",
    "method": "mostrarProducto",
    "params": {
      "id": "number"
    }
  },
  "GuriZone/login": {
    "controller": "Usuario",
    "method": "login"
  },
  "GuriZone/logout": {
    "controller": "Usuario",
    "method": "logout"
  },
  "GuriZone/dashboard": {
    "controller": "Usuario",
    "method": "dashboard"
  },
  "GuriZone/dashboard/gestion": {
    "controller": "Usuario",
    "method": "gestion"
  },
  "GuriZone/dashboard/producto/crear": {
    "controller": "Producto",
    "method": "crearProducto"
  },
  "GuriZone/dashboard/producto/editar/:id": {
    "controller": "Producto",
    "method": "editarProducto",
    "params": {
      "id": "number"
    }
  },
  "GuriZone/dashboard/producto/borrar/:id": {
    "controller": "Producto",
    "method": "borrarProducto",
    "params": {
      "id": "number"
    }
  },
  "GuriZone/mi_perfil": {
    "controller": "Usuario",
    "method": "perfil"
  },
  "GuriZone/contacto": {
    "controller": "Usuario",
    "method": "contactanos"
  },
  "GuriZone/registrarse": {
    "controller": "Usuario",
    "method": "registrarse"
  }
}