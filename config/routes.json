{
  "GuriZone/": {
    "controller": "Producto",
    "method": "index"
  },
  "GuriZone/tienda/:categoria/:page": {
    "controller": "Producto",
    "method": "mostrarPorCategoria",
    "params": {
      "categoria": "string",
      "page": "number"
    }
  },
  "GuriZone/tienda/:fecha_inicial/:fecha_final/:page": {
    "controller": "Producto",
    "method": "mostrarPorFecha",
    "params": {
      "fecha_inicial": "string",
      "fecha_final": "string",
      "page": "number"
    }
  },
  "GuriZone/tienda/:search/:page": {
    "controller": "Producto",
    "method": "mostrarPorBusqueda",
    "params": {
      "search": "string",
      "page": "number"
    }
  },
  "GuriZone/producto/:id": {
    "controller": "Producto",
    "method": "mostrarProducto",
    "params": {
      "id": "number"
    }
  },
  "GuriZone/login": {
    "controller": "Usuario",
    "method": "login"
  },
  "GuriZone/logout": {
    "controller": "Usuario",
    "method": "logout"
  },
  "GuriZone/dashboard": {
    "controller": "Usuario",
    "method": "dashboard"
  },
  "GuriZone/dashboard/gestion": {
    "controller": "Usuario",
    "method": "gestion"
  },
  "GuriZone/dashboard/producto/crear": {
    "controller": "Producto",
    "method": "crearProducto"
  },
  "GuriZone/dashboard/editar/:id": {
    "controller": "Producto",
    "method": "editarProducto",
    "params": {
      "id": "number"
    }
  },
  "GuriZone/dashboard/borrar/:id": {
    "controller": "Producto",
    "method": "borrarProducto",
    "params": {
      "id": "number"
    }
  },
  "GuriZone/mi_perfil": {
    "controller": "Usuario",
    "method": "perfil"
  },
  "GuriZone/contacto": {
    "controller": "Usuario",
    "method": "contactanos"
  }
}